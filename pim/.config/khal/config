[calendars]

[[personal]]
path=~/.calendars/personal/personal/
priority = 10
color = dark red

[[cutateous]]
path=~/.calendars/personal/cutateous/
priority = 9
color = dark blue

[[contact_birthdays]]
path=~/.calendars/personal/contact_birthdays/
readonly = True
priority = 1
color = light green

[default]
default_calendar = personal
timedelta = 3d

[locale]
timeformat = %H:%M
dateformat= %d.%m.%Y
longdateformat= %d日%m月%Y
datetimeformat= %d.%m.%Y %H:%M
longdatetimeformat= %d.%m.%Y %H:%M

[view]
frame = top
